# Filter for ubuntu build
#
# PBDEBSTD is replaced by the Debian standard version
filter PBDEBSTD = 3.7.3

# PBDEBCOMP is replaced by the Debian Compatibility value
filter PBDEBCOMP = 4

filter PBBOOST = libboost-dev, libboost-thread-dev, libboost-program-options-dev
filter PBTEX = texlive-binaries
filter PB_LIBDIGEST_SHA_PERL = libdigest-sha-perl
